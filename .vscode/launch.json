{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/buffer.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "Python: Run",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/client.py",
            "cwd": "${workspaceFolder}",
            "gevent": true,
            "stopOnEntry": false,
            "justMyCode": false,
            "console": "integratedTerminal",
            "pythonPath": "${config:python.interpreterPath}"
        },
    ]
}